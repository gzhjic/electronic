    001D9 F39C      BLT	0x01CD
(0069) Hign_24L01_CSN;                               
    001DA 9A2C      SBI	0x05,4
(0070) return(status1);                    // return nRF24L01 status uchar 
    001DB 2D0A      MOV	R16,R10
    001DC 940C 025D JMP	pop_xgset30FC
