#include "uart.h"
#include "delay.h"

void main(void)
{
	UART1_Init();
	UART0_Init();
	while(1)
	{
		SREG&=~(1<<7);
		UART1_Send_Char("ASDGFHFSAD");
		UART0_Send_Char("ASDGFHFSAD");
		SREG|=(1<<7);
		delay_ms(500);
	}
}


#pragma interrupt_handler UART1:29
void UART1(void)
{
	uchar data;
	SREG&=~(1<<7);
	data=UDR0;
	UCSR1A|=(1<<UDRE1);
	UART0_Send('6');
	SREG|=(1<<7);
}